{
    "env": {
        "node": true,
        "commonjs": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": ["error", "windows"],
      "indent": 0
    }
}